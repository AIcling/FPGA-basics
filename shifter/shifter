#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55d89e0a10f0 .scope module, "shifter_tb" "shifter_tb" 2 2;
 .timescale -9 -9;
v0x55d89e0b39d0_0 .var "clk", 0 0;
v0x55d89e0b3a90_0 .var "clr", 0 0;
v0x55d89e0b3b60_0 .var "din", 0 0;
v0x55d89e0b3c60_0 .net "dout", 3 0, v0x55d89e0b3840_0;  1 drivers
S_0x55d89e0a1280 .scope module, "inst" "shifter" 2 5, 3 1 0, S_0x55d89e0a10f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "din";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "clr";
    .port_info 3 /OUTPUT 4 "dout";
v0x55d89e068bf0_0 .net "clk", 0 0, v0x55d89e0b39d0_0;  1 drivers
v0x55d89e069000_0 .net "clr", 0 0, v0x55d89e0b3a90_0;  1 drivers
v0x55d89e0b3770_0 .net "din", 0 0, v0x55d89e0b3b60_0;  1 drivers
v0x55d89e0b3840_0 .var "dout", 3 0;
E_0x55d89e0675b0 .event posedge, v0x55d89e068bf0_0;
    .scope S_0x55d89e0a1280;
T_0 ;
    %wait E_0x55d89e0675b0;
    %load/vec4 v0x55d89e069000_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55d89e0b3840_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55d89e0b3840_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %assign/vec4 v0x55d89e0b3840_0, 0;
    %load/vec4 v0x55d89e0b3770_0;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x55d89e0b3840_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d89e0a10f0;
T_1 ;
    %vpi_call 2 12 "$dumpfile", "shifter.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d89e0a10f0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55d89e0a10f0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d89e0b39d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d89e0b3a90_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d89e0b3a90_0, 0, 1;
    %pushi/vec4 100, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d89e0b3b60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d89e0b3b60_0, 0, 1;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %delay 1000, 0;
    %vpi_call 2 24 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55d89e0a10f0;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x55d89e0b39d0_0;
    %inv;
    %store/vec4 v0x55d89e0b39d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "shifter_tb.v";
    "shifter.v";
